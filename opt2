def _load_lut_mapping_high(self):
    """
    ì‹¤í–‰ py íŒŒì¼ í´ë” ë‚´ 'LUT_index_mapping.csv'ë¥¼ ì½ì–´
    gray(0~255) â†’ 12bit LUT index ë§¤í•‘ ë°°ì—´ì„ ë¡œë“œ.

    CSV ì˜ˆì‹œ:
        8bit gray,12bit LUT index
        0,0
        1,16
        ...
        255,4095

    ëª¨ë“  ì±„ë„(R/G/B)ì´ ë™ì¼í•œ ë§¤í•‘ì„ ì‚¬ìš©í•˜ëŠ” ê²ƒìœ¼ë¡œ ê°€ì •.
    """
    if hasattr(self, "_lut_map_high") and self._lut_map_high is not None:
        return  # ì´ë¯¸ ë¡œë“œë¨

    # ğŸ”¹ CSV ê²½ë¡œ: ì‹¤í–‰ ì¤‘ py íŒŒì¼ ê¸°ì¤€ ê°™ì€ í´ë”
    csv_path = os.path.join(os.path.dirname(__file__), "LUT_index_mapping.csv")
    if not os.path.exists(csv_path):
        raise FileNotFoundError(f"[LUT MAP] LUT_index_mapping.csv not found: {csv_path}")

    # ğŸ”¹ CSV ì½ê¸°
    df = pd.read_csv(csv_path)

    # ğŸ”¹ ì»¬ëŸ¼ëª… ì •ê·œí™” (í˜¹ì‹œ ê³µë°±/ëŒ€ì†Œë¬¸ì ë‹¬ë¼ë„ ëŒ€ì‘)
    df.columns = [c.strip().lower() for c in df.columns]
    if not ("8bit gray" in df.columns and "12bit lut index" in df.columns):
        raise ValueError(f"[LUT MAP] Unexpected columns: {df.columns}")

    # ğŸ”¹ numpy ë°°ì—´ë¡œ ë³€í™˜
    gray_col = df["8bit gray"].to_numpy(dtype=np.int32)
    idx_col  = df["12bit lut index"].to_numpy(dtype=np.int32)

    # ğŸ”¹ gray ìˆœì„œëŒ€ë¡œ ì •ë ¬ (í˜¹ì‹œ ì„ì—¬ ìˆìœ¼ë©´)
    sort_idx = np.argsort(gray_col)
    idx_col_sorted = idx_col[sort_idx]

    # ğŸ”¹ ëª¨ë“  ì±„ë„ì— ë™ì¼ ë§¤í•‘ ì ìš©
    self._lut_map_high = {
        "R": idx_col_sorted,
        "G": idx_col_sorted,
        "B": idx_col_sorted,
    }

    logging.info(f"[LUT MAP] loaded {csv_path}, shape={df.shape}, range={idx_col_sorted.min()}~{idx_col_sorted.max()}")